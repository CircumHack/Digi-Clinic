                orientation: 'vertical'        
        MDLabel:
            text:'Member(s)'
            size_hint:.7,.05
            pos_hint:{'center_x':.5,'center_y':.5}        
        RecycleView:
            id:member_rv
            key_viewclass: 'member'
            key_size: 'height'  
            size_hint:.7,.5
            pos_hint:{'center_x':.5,'center_y':.5}                              
            RecycleBoxLayout:
                padding: dp(10)
                spacing:dp(10)
                default_size: None, dp(40)
                default_size_hint: 1, None
                size_hint_y: None
                height: self.minimum_height
                orientation: 'vertical'    
        MDFillRoundFlatButton:
            id:create
            on_release:app.root.change_screen('create_fam_wallet')
            text:'Create'
            size_hint:.9,.25
            pos_hint:{'center_x':.5,'center_y':.5}        

<SavingsScheme@MDScreen>:
    ScrollView:
        size_hint:.9,.95
        padding:[20,10,10,20]
        do_scroll:False,True
        pos_hint:{'center_x':.5,'center_y':.5}
        MDBoxLayout:
            orientation:'vertical'
            size_hint:1,1.5
            spacing:dp(10)
            pos_hint:{'center_x':.5}            
            MDToolbar:
                title:'Savings Scheme'
                md_bg_color:app.bg_color
                pos_hint:{'top':1}
                left_action_items:[['chevron-left',lambda _:app.root.back_screen('new_wallet')]]        
            MDLabel:
                text:'How much would you like to save (NGN)?'
                size_hint:.9,.1                
                pos_hint:{'center_x':.5,'center_y':.5}
            MDBoxLayout:            
                spacing:dp(10)
                size_hint:.95,.35                
                pos_hint:{'center_x':.5,'center_y':.5}
                Options:
                    id:option1                    
                    text:'1000'
                    value:1000
                    on_release:app.root.option_choose(self,root.ids.option2,root.ids.option3,root.ids.option4,root.ids.customcost)
                Options:
                    id:option2
                    text:'2000'
                    value:2000
                    on_release:app.root.option_choose(self,root.ids.option1,root.ids.option3,root.ids.option4,root.ids.customcost)
                Options:
                    id:option3
                    text:'5000'
                    value:5000
                    on_release:app.root.option_choose(self,root.ids.option1,root.ids.option2,root.ids.option4,root.ids.customcost)
                Options:
                    id:option4
                    text:'10000'
                    value:10000
                    on_release:app.root.option_choose(self,root.ids.option1,root.ids.option2,root.ids.option3,root.ids.customcost)
            MDBoxLayout:            
                spacing:dp(10)     
                size_hint:.65,.35                           
                pos_hint:{'left':1,'center_y':.5}
                Options:
                    id:customcost
                    text:'Custom'
                    value:0
                    textfield:root.ids.camount
                    size_hint:.3,.8                    
                    on_release:
                        app.root.option_choose(self,root.ids.option1,root.ids.option2,root.ids.option3,root.ids.option4)
                        app.root.custom_open(self,root.ids.camount)
                CupertinoTextField:
                    id:camount
                    hint_text:'Enter Custom Amount'
                    input_filter:'int'
                    size_hint:.7,.8                    
                    disabled:True
            MDLabel:
                text:'How often?'
                size_hint:.7,.1                    
                pos_hint:{'center_x':.5,'center_y':.5}
            MDBoxLayout:
                orientation:'horizontal'
                spacing:dp(10)
                size_hint:.95,.35  
                pos_hint:{'center_x':.5,'center_y':.5}
                Options:
                    id:daily                   
                    text:'Daily'
                    value:1
                    on_release:app.root.option_daily(self,root.ids.weekly,root.ids.monthly,root.ids.once)
                Options:
                    id:weekly
                    text:'Weekly'
                    value:7
                    on_release:app.root.option_daily(self,root.ids.daily,root.ids.monthly,root.ids.once)
                Options:
                    id:monthly
                    text:'Monthly'
                    value:31
                    on_release:app.root.option_daily(self,root.ids.weekly,root.ids.daily,root.ids.once)
                Options:
                    id:once
                    text:'One Off'
                    value:0
                    on_release:app.root.option_daily(self,root.ids.weekly,root.ids.monthly,root.ids.daily)
            MDLabel:
                text:'For how long?'
                size_hint:.5,.1
                pos_hint:{'center_x':.5,'center_y':.5}
            MDBoxLayout:
                orientation:'horizontal'
                spacing:dp(10)
                size_hint:.95,.35  
                pos_hint:{'center_x':.5,'center_y':.5}
                Options:
                    id:month3                    
                    text:'3 Months'
                    value:3
                    on_release:app.root.option_choose(self,root.ids.month6,root.ids.month9,root.ids.customtime)
                Options:
                    id:month6
                    text:'6 Months'
                    value:6
                    on_release:app.root.option_choose(self,root.ids.month3,root.ids.month9,root.ids.customtime)
                Options:
                    id:month9
                    text:'9 Months'
                    value:9
                    on_release:app.root.option_choose(self,root.ids.month3,root.ids.month6,root.ids.customtime)
            MDBoxLayout:            
                spacing:dp(10)                
                size_hint:.65,.35  
                pos_hint:{'left':1,'center_y':.5}
                Options:
                    id:customtime
                    text:'Custom'
                    value:0
                    textfield:root.ids.ctime
                    size_hint:.3,.8
                    on_release:
                        app.root.option_choose(self,root.ids.month3,root.ids.month6,root.ids.month9)
                        app.root.custom_open(self,root.ids.ctime)
                CupertinoTextField:
                    id:ctime
                    hint_text:'Enter Custom Duration (Months)'
                    size_hint:.7,.8
                    disabled:True
                    input_filter:'int'
            MDFillRoundFlatButton:
                id:create
                text:'Create'
                size_hint:.9,.3
                pos_hint:{'center_x':.5,'center_y':.5}
                on_release:app.root.change_screen('bioregister')
<Family@MDScreen>:
    MDBoxLayout:
        orientation:'vertical'
        size_hint:1,1
        MDBoxLayout:
            orientation:'horizontal'
            MDIconButton:
                id:back
                icon:'chevron-left'
