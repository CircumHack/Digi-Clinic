DigiManager:

    NoInternet:
        id:nointernet
        name:'nointernet'
    Login:
        id:login
        name:'login'
    
    Home:
        id:home
        name:'home'
        HomeDrawer:
            id:home_drawer
            state:'close'
            color:1,1,1,1
            radius:[0,0,20,0]
            size_hint_y:1
            padding:[15,35,10,10]
            scrim_color:0,0,0,.75
            opening_time:.1
            closing_time:.1
            size_hint_x:.75
            on_leave:self.state='close'
            MDBoxLayout: 
                id:content
                orientation:'vertical'
                size_hint:1,1
                padding:dp(5)
                spacing:dp(5)
                pos_hint:{'top':1,'center_y':.5}
    Picture:
        id:picture
        name:'picture'
    
    Personal:
        id:personal
        name:'personal'

    ForgotPass:
        id:forgotpass
        name:'forgotpass'

    ChangePassword:
        id:changepass
        name:'changepass'

    Email:
        id:email
        name:'email'

    Phone:
        id:phone
        name:'phone'

    BioRegister:
        id:bioregister
        name:'bioregister'

    New_Wallet:
        id:new_wallet
        name:'new_wallet'

    SavingsScheme:
        id:savings
        name:'savings'
    
    IndividualWallet:
        id:ind_wallet
        name:'ind_wallet'
    
    FamilyWallet:
        id:fam_wallet
        name:'fam_wallet'
    
    Notifications:
        id:notification
        name:'notification'

    WalletHome:
        id:wallethome
        name:'wallethome'
        
<NoInternet@MDScreen>:
    MDBoxLayout:
        size_hint:1,.45
        orientation:'vertical'
        spacing:5 
        halign:'center'
        valign:'center'
        pos_hint:{'center_x':.5,'center_y':.5}
        MDIconButton:
            icon:'cloud-off-outline'
            user_font_size:'70sp'
            halign:'center'
            pos_hint:{'center_x':.5}
        MDLabel:
            text:'Unable to connect to database.\nCheck your Internet Connection and try again'
            font_size:16 
            halign:'center'
            size_hint:.9,.5
            pos_hint:{'center_x':.5}        

<Login@MDScreen>:
    MDBoxLayout:
        orientation:'vertical'
        size_hint:1,1
        padding:[0,0,0,10]
        pos_hint:{'center_x':.5,'center_y':.5}
        FitImage:
            source:'pics/login.png'
            size_hint:1,.5
            radius:0,0,15,15
            pos_hint:{'center_x':.5,'center_y':.5}
        MDBoxLayout:
            size_hint:1,.5
            spacing:dp(15)
            orientation:'vertical'
            pos_hint:{'center_x':.5,'center_y':.5}
            MDLabel:
                text:'Welcome'
                size_hint:.6,.15
                pos_hint:{'center_x':.5,'center_y':.5}
            CupertinoTextField:
                id:user_email
                size_hint:.6,.18
                hint_text:'Username / Email'
                pos_hint:{'center_x':.5,'center_y':.5}
            PasswordText:
                id:password
                size_hint:.6,.19
                pos_hint:{'center_x':.5,'center_y':.5}
            MDFillRoundFlatButton:
                id:login
                size_hint:.6,.2
                text:'Login'
                pos_hint:{'center_x':.5,'center_y':.5}
                on_release:app.root.change_screen('home')
            MDFlatButton:
                id:forgotpass
                size_hint:.6,.1
                text:'Forgot Password?'
                on_release:app.root.change_screen('forgotpass')
                pos_hint:{'center_x':.5,'center_y':.5}
                font_size:14
            MDFlatButton:
                id:noaccount:
                size_hint:.6,.1
                text:"Don't have an Account?"
                on_release:app.root.change_screen('personal')
                pos_hint:{'center_x':.5,'center_y':.5}
                font_size:14
<Personal@MDScreen>:
    ScrollView:
        size_hint:.75,.9
        padding:[20,10,10,20]
        do_scroll:False,True
        pos_hint:{'center_x':.5,'center_y':.5}
        MDBoxLayout:
            orientation:'vertical'
            size_hint:1,1.9
            pos_hint:{'center_x':.5}
            spacing:dp(10)            
            MDToolbar:
                title:'Peronal Information'
                md_bg_color:app.bg_color
                pos_hint:{'top':1}
                left_action_items:[['chevron-left',lambda _:app.root.back_screen('login')]]        
            MDLabel:
                text:'Personal Details'
                font_size:13
                pos_hint:{'center_x':.5,'center_y':.5}
                size_hint:.7,.2
            CupertinoTextField:
                id:fname
                hint_text:'First Name'
                size_hint:.9,.2
                pos_hint:{'center_x':.5,'center_y':.5}
            CupertinoTextField:
                id:lname
                hint_text:'Last Name'
                size_hint:.9,.2
                pos_hint:{'center_x':.5,'center_y':.5}
            CupertinoTextField:
                id:oname
                hint_text:'Other Name(s)'
                size_hint:.9,.2
                pos_hint:{'center_x':.5,'center_y':.5}
            MDLabel:
                text:'Gender'
                font_size:13
                pos_hint:{'center_x':.5,'center_y':.5}
                size_hint:.6,.1
            MDBoxLayout:
                orientation:'horizontal'
                size_hint:.9,.2
                Choice:
                    id:male
                    text:'Male'
                    group:'gender'
                Choice:
                    id:female
                    text:'Female'
                    group:'gender'
            MDFillRoundFlatIconButton:
                id:dob
                icon:'calendar'
                text:'Select Date of Birth'
                size_hint:.9,.23
                pos_hint:{'center_x':.5,'center_y':.5}
                date:None
                on_release:app.root.open_dob()
            CodeNum:
                id:nin
                hint_text:'National Identification Number'
                size_hint:.9,.2
                input_filter:'int'
                max_text_length:11
                pos_hint:{'center_x':.5,'center_y':.5}
            MDLabel:
                text:'Contact Details'
                font_size:13
                pos_hint:{'center_x':.5,'center_y':.5}
                size_hint:.7,.2
            CupertinoTextField:
                id:email
                hint_text:'Email Address'
                size_hint:.9,.2
                pos_hint:{'center_x':.5,'center_y':.5}
            MDBoxLayout:
                orientation:'horizontal'
                size_hint:.9,.2
                spacing:dp(10)
                MDIconButton:
                    id:iso
                    icon:'pics/ng.png'
                    user_font_size:'8sp'
                    value:234
                    size_hint_x:.2
                    pos_hint:{'center_y':.5}
                CodeNum:
                    id:phone
                    input_filter:'int'
                    max_text_length:14
                    size_hint_x:.7
                    hint_text:'Phone Number'
                    pos_hint:{'center_y':.5}
            PasswordText:
                id:password
                size_hint:.9,.2
                pos_hint:{'center_x':.5,'center_y':.5}
            CupertinoTextField:
                id:confirm_password
                password:True
                hint_text:'Confirm Password'
                size_hint:.9,.2
                pos_hint:{'center_x':.5,'center_y':.5}
            MDLabel:
                text:'Home Address'
                font_size:13
                pos_hint:{'center_x':.5,'center_y':.5}
                size_hint:.7,.2                
            CupertinoTextField:
                id:street
                hint_text:'Street Line'
                size_hint:.9,.4
                multiline:True
                pos_hint:{'center_x':.5,'center_y':.5}
            CupertinoTextField:
                id:state
                hint_text:'State'
                size_hint:.9,.2
                pos_hint:{'center_x':.5,'center_y':.5}
            MDFillRoundFlatIconButton:
                id:country
                icon:'earth'
                text:'Nigeria'
                size_hint:.9,.23
                pos_hint:{'center_x':.5,'center_y':.5}
            MDFillRoundFlatButton:
                id:next
                text:'Next'
                size_hint:.9,.25
                pos_hint:{'center_x':.5,'center_y':.5}
                on_release:app.root.change_screen('picture')
<Picture@MDScreen>:
    MDBoxLayout:
        orientation:'vertical'
        size_hint:.8,.85
        pos_hint:{'center_x':.5,'center_y':.5}
        MDToolbar:
            title:'Upload Picture'
            md_bg_color:app.bg_color
            pos_hint:{'top':1}
            left_action_items:[['chevron-left',lambda _:app.root.back_screen('personal')]] 
        MDLabel:
            text:'Click Image below to  recent Picture of yourself.'
            pos_hint:{'center_x':.5,'center_y':.5}
        UploadPhoto:
            id:image
            source:'pics/account.png'
            radius:[15]
            size_hint:.8,.8
            pos_hint:{'center_x':.5,'center_y':.5}
            on_release:app.root.choose_photo()            
        MDLabel:
            text:'Image should not be older than 6 month of current appearance'
            pos_hint:{'center_x':.5,'center_y':.5}
        MDFillRoundFlatButton:
            id:upload
            text:'Upload'
            size_hint:.9,.25
            pos_hint:{'center_x':.5,'center_y':.5}
            on_release:app.root.change_screen('email')
<Email@MDScreen>:
    MDToolbar:
        title:'Verify Email'
        md_bg_color:app.bg_color
        pos_hint:{'top':1}
        left_action_items:[['chevron-left',lambda _:app.root.change_screen('PHONE')]]        
    MDCard:
        size_hint:.8,.6        
        radius:[15]
        padding:20,20,20,20
        spacing:10
        pos_hint:{'center_x':.5,'center_y':.6}
        MDGridLayout:                    
            cols:1
            spacing:15
            padding:30,20,30,20
            size_hint:.9,1
            pos_hint:{'center_x':.5,'center_y':.5}
            MDLabel:
                text:'Enter Verification Code sent to your Email'
                font_size:14
                size_hint_x:.9
                halign:'center'
            CupertinoTextField:
                id:verifycod
                hint_text:'Verification Code'
                size_hint:.9,.25
                halign:'center'                 
            CupertinoButton:
                id:verify
                text:'Verify'
                size_hint:.4,.2
                on_release:app.root.change_screen('phone')
                halign:'center'            
<Phone@MDScreen>:
    ##md_bg_color:app.back_light
    MDToolbar:
        title:'Verify Phone Number'
        md_bg_color:app.bg_color
        pos_hint:{'top':1}
        left_action_items:[['chevron-left',lambda _:app.root.change_screen('new_wallet')]]
    MDCard:
        size_hint:.8,.6        
        radius:[15]
        padding:20,20,20,20
        spacing:10
        pos_hint:{'center_x':.5,'center_y':.6}
        MDGridLayout:                    
            cols:1
            spacing:15
            padding:30,20,30,20
            size_hint:.9,1
            pos_hint:{'center_x':.5,'center_y':.5}
            MDLabel:
                text:'Enter Verification Code sent to your Phone Number'
                font_size:14
                size_hint_x:.9
                halign:'center'
            CupertinoTextField:
                id:verifycod
                hint_text:'Verification Code'
                size_hint:.9,.4                 
                halign:'center'
            CupertinoButton:
                id:verify
                text:'Verify'
                size_hint:.5,.3
                on_release:app.root.change_screen('new_wallet')            
<New_Wallet@MDScreen>:
    MDBoxLayout:
        orientation:'vertical'
        spacing:dp(20)
        size_hint:.8,.6
        pos_hint:{'center_x':.5,'center_y':.5}
        MDToolbar:
            title:'Create Wallet'
            md_bg_color:app.bg_color
            pos_hint:{'top':1}
            left_action_items:[['chevron-left',lambda _:app.root.back_screen('picture')]]        
        MDCard:
            id:individual
            size_hint:1,.5            
            radius:15
            md_bg_color:1,1,1,1
            pos_hint:{'center_x':.5,'center_y':.5}
            on_release:app.root.change_screen('ind_wallet')
            MDBoxLayout:                
                orientation:'horizontal'
                size_hint:1,1
                padding:20,20,20,20
                MDLabel:
                    text:'Individual'
                    size_hint:.8,.6
                    pos_hint:{'center_x':.5,'center_y':.5}
                MDIcon:
                    id:icon_ind
                    size_hint:.2,.6
                    icon:'chevron-right'
                    pos_hint:{'center_x':.5,'center_y':.5}
        MDCard:
            id:family
            size_hint:1,.5 
            radius:15
            md_bg_color:1,1,1,1           
            pos_hint:{'center_x':.5,'center_y':.5}
            on_release:app.root.change_screen('fam_wallet')
            MDBoxLayout:                
                orientation:'horizontal'
                size_hint:1,1
                padding:20,20,20,20
                MDLabel:
                    text:'Family (Optional)'
                    size_hint:.8,.6
                    pos_hint:{'center_x':.5,'center_y':.5}
                MDIcon:
                    id:icon_fam
                    icon:'chevron-right'   
                    size_hint:.2,.3     
                    pos_hint:{'center_x':.5,'center_y':.5}
        MDFillRoundFlatButton:
            id:create
            on_release:app.root.change_screen('savings')
            text:'Next'
            size_hint:.9,.425
<IndividualWallet@MDScreen>:
    MDBoxLayout:
        orientation:'vertical'
        spacing:dp(20)
        size_hint:.8,.6
        pos_hint:{'center_x':.5,'center_y':.5}
        MDToolbar:
            title:'Individual Wallet'
            md_bg_color:app.bg_color
            pos_hint:{'top':1}
            left_action_items:[['chevron-left',lambda _:app.root.back_screen('new_wallet')]]        
        MDLabel:
            id:name
            #text:f'Name: {app.root.ids.personal.ids.fname.strip()} {app.root.ids.personal.ids.lname.strip()} {app.root.ids.personal.ids.oname.strip()}'
            size_hint:.9,.45
            pos_hint:{'center_x':.5,'center_y':.5}
        MDLabel:
            text:'Enter your BVN and a secret 4 digit Wallet Pin below'
            size_hint:.9,.2
            pos_hint:{'center_x':.5,'center_y':.5}
        CodeNum:
            id:bvn
            hint_text:'BVN'
            size_hint:.7,.3            
            input_filter:'int'
            max_text_length:11
            pos_hint:{'center_x':.5,'center_y':.5}
        CodeNum
            id:pin
            hint_text:'Wallet Pin'
            size_hint:.7,.3            
            input_filter:'int'
            max_text_length:4
            pos_hint:{'center_x':.5,'center_y':.5}
        MDFillRoundFlatButton:
            id:create
            on_release:app.root.change_screen('create_ind_wallet')
            text:'Create'
            size_hint:.9,.3   
            pos_hint:{'center_x':.5,'center_y':.5}
<FamilyWallet@MDScreen>:
    MDBoxLayout:
        orientation:'vertical'
        spacing:dp(20)
        size_hint:.8,.9
        pos_hint:{'center_x':.5,'center_y':.5}
        MDToolbar:
            title:'Family Wallet'
            md_bg_color:app.bg_color
            pos_hint:{'top':1}
            left_action_items:[['chevron-left',lambda _:app.root.back_screen('new_wallet')]]        
        CupertinoTextField:
            id:fam_name
            hint_text:'Family Name'   
            size_hint:.7,.2 
            pos_hint:{'center_x':.5,'center_y':.5}            
        CodeNum:
            id:pin
            hint_text:'Enter Wallet Pin'            
            size_hint:.7,.2 
            input_filter:'int'
            max_text_length:4
            pos_hint:{'center_x':.5,'center_y':.5}        
        MDLabel:
            text:'Administrator(s)'
            size_hint:.7,.05
            pos_hint:{'center_x':.5,'center_y':.5}        
        RecycleView:
            id: admin_rv
            key_viewclass: 'admin'
            key_size: 'height'                        
            size_hint:.7,.5 
